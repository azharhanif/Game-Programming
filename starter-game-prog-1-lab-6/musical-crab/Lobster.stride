                        <call expression="world.removeObject(crab)" expression-java="world.removeObject(crab)" enable="true"/>
                        <call expression="Greenfoot.playSound(&quot;Alien Creak1.wav&quot;)" expression-java="Greenfoot.playSound(&quot;Alien Creak1.wav&quot;)" enable="true"/>
                    </ifStatements>
                </if>
            </body>
        </method>
